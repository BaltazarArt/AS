<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".contactoFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".contactos">

        <EditText
            android:id="@+id/editTextTextEmailAddress"
            android:layout_width="338dp"
            android:layout_height="37dp"
            android:ems="10"
            android:hint="Correo"
            android:inputType="textEmailAddress"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="34dp"
            tools:layout_editor_absoluteY="539dp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="71dp"
            android:layout_height="65dp"
            app:srcCompat="@mipmap/logo"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="16dp" />

        <com.google.android.gms.maps.MapView
            android:id="@+id/mapView"
            android:layout_width="311dp"
            android:layout_height="80dp"
            android:visibility="visible"
            tools:layout_editor_absoluteX="50dp"
            tools:layout_editor_absoluteY="385dp"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="232dp"
            android:layout_height="57dp"
            android:fontFamily="sans-serif-black"
            android:text="Contactanos"
            android:textColor="#000000"
            android:textSize="36sp"
            tools:layout_editor_absoluteX="112dp"
            tools:layout_editor_absoluteY="24dp" />

        <EditText
            android:id="@+id/editTextTextPersonName4"
            android:layout_width="338dp"
            android:layout_height="80dp"
            android:ems="10"
            android:hint="Mensaje"
            android:inputType="textPersonName"
            tools:layout_editor_absoluteX="34dp"
            tools:layout_editor_absoluteY="588dp" />

        <Button
            android:id="@+id/button4"
            android:layout_width="201dp"
            android:layout_height="41dp"
            android:background="@drawable/custom_button"
            android:backgroundTint="@null"
            android:text="Enviar"
            android:textColor="#000000"
            tools:layout_editor_absoluteX="111dp"
            tools:layout_editor_absoluteY="674dp" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@mipmap/telefono"
            tools:layout_editor_absoluteX="47dp"
            tools:layout_editor_absoluteY="130dp" />

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@mipmap/correo"
            tools:layout_editor_absoluteX="47dp"
            tools:layout_editor_absoluteY="240dp" />

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="87dp"
            android:layout_height="83dp"
            app:srcCompat="@mipmap/reloj"
            tools:layout_editor_absoluteX="257dp"
            tools:layout_editor_absoluteY="130dp" />

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@mipmap/ubicacion"
            tools:layout_editor_absoluteX="256dp"
            tools:layout_editor_absoluteY="240dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="442.000.0000"
            android:textSize="10sp"
            tools:layout_editor_absoluteX="60dp"
            tools:layout_editor_absoluteY="220dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="125dp"
            android:layout_height="22dp"
            android:text="Circuito Puerta del Sol, Angel Aniel 701"
            android:textSize="8sp"
            tools:layout_editor_absoluteX="227dp"
            tools:layout_editor_absoluteY="334dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="10:00 am to 22:00 pm"
            android:textSize="8sp"
            tools:layout_editor_absoluteX="266dp"
            tools:layout_editor_absoluteY="220dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="101dp"
            android:layout_height="21dp"
            android:text="Contacto@Astros.com  ventas_astros@Astros.com"
            android:textSize="8sp"
            tools:layout_editor_absoluteX="47dp"
            tools:layout_editor_absoluteY="335dp" />

        <EditText
            android:id="@+id/editTextTextPersonName9"
            android:layout_width="338dp"
            android:layout_height="35dp"
            android:ems="10"
            android:hint="Nombre"
            android:inputType="textPersonName"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="34dp"
            tools:layout_editor_absoluteY="479dp" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="1dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>